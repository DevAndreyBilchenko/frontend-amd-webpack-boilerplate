{
  "name": "andrey_bilchenko_frontend_boilerblate",
  "version": "1.0.0",
  "description": "fast start new project with webpack",
  "main": "index.js",
  "install": "npm i && npm run dev",
  "scripts": {
    "dev": "node node_modules/webpack/bin/webpack.js serve --config webpack.config.js --color --mode development",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --color --progress --mode development",
    "prod": "node node_modules/webpack/bin/webpack.js --config webpack.config.js --color --progress --mode production",
    "sass_lint": "node node_modules/.bin/sass-lint -c .sass-lint.yml -v",
    "sass_lint:fix": "sass-lint-auto-fix -c .sass-lint.yml",
    "js_lint": "node node_modules/.bin/eslint ./dev/scripts/module/**"
  },
  "author": "Andrey Bilchenko",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.1",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.1.0",
    "raw-loader": "^4.0.2",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.2.1",
    "twig-html-loader": "^0.1.8",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {}
}
